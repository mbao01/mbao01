{
  "name": "@mbao01/next",
  "version": "0.0.4",
  "private": false,
  "type": "module",
  "author": "Ayomide Bakare",
  "license": "MIT",
  "description": "A fully ready-to-consume Next.js component and utility library",
  "keywords": [
    "react",
    "components",
    "ui",
    "library",
    "nextjs"
  ],
  "main": "./src/index.ts",
  "types": "dist/types/index.d.ts",
  "files": [
    "src",
    "dist/types"
  ],
  "bugs": "https://github.com/mbao01/mbao01/issues",
  "homepage": "https://next.ayomidebakare.site",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbao01/mbao01.git"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "next build",
    "build:declaration": "tsc --jsx preserve --declaration --esModuleInterop --emitDeclarationOnly ./src/index.ts --outDir dist/types",
    "build-storybook": "storybook build -o ../../docs/storybook/next",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "next dev",
    "start": "next start",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@mbao01/common": "workspace:^",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
