{
  "name": "@mbao01/next",
  "version": "0.7.0",
  "private": false,
  "type": "module",
  "author": "Ayomide Bakare",
  "license": "MIT",
  "description": "A fully ready-to-consume Next.js component and utility library",
  "keywords": [
    "react",
    "components",
    "ui",
    "library",
    "nextjs"
  ],
  "main": "./src/index.ts",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "src",
    "dist/types"
  ],
  "bugs": "https://github.com/mbao01/mbao01/issues",
  "homepage": "https://ui.ayomidebakare.site/next/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbao01/mbao01.git"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "clean": "pnpx rimraf node_modules",
    "dev": "storybook dev -p 6007",
    "build": "next build",
    "postbuild": "tsc --project tsconfig.declaration.json",
    "build-storybook": "storybook build -o ../../docs/storybook/next",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "preview": "next dev",
    "prod": "pnpx http-server ../../docs/storybook/next --port 6007",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:visual": "test-storybook --url http://127.0.0.1:6007",
    "test:visual:coverage": "test-storybook --coverage --url http://127.0.0.1:6007",
    "test:visual:ci": "test-storybook --ci --verbose --url http://127.0.0.1:6007",
    "test:visual:watch": "test-storybook --watch --url http://127.0.0.1:6007",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@mbao01/common": "workspace:^",
    "@storybook/nextjs-vite": "^10.1.11",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-coverage": "^3.0.0",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/test-runner": "^0.24.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.2.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "^16.5.0",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^27.4.0",
    "next": "16.1.1",
    "next-router-mock": "^1.0.4",
    "path": "^0.12.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "^7.11.0",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "next": "16"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
